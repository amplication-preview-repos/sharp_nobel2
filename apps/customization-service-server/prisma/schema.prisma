datasource db {
  provider = "postgresql"
  url      = env("DB_URL")
}

generator client {
  provider = "prisma-client-js"
}

model Font {
  createdAt DateTime @default(now())
  id        String   @id @default(cuid())
  name      String?
  updatedAt DateTime @updatedAt
  url       String?
}

model Template {
  createdAt   DateTime @default(now())
  css         String?
  description String?
  html        String?
  id          String   @id @default(cuid())
  name        String?
  updatedAt   DateTime @updatedAt
}
